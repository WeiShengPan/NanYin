#set($pageJS='
<script>
    $(".sidebar-menu li.treeview>a:contains(用户管理)").parent().addClass("active");
</script>')
 <section class="content-header">
     <h1>
         用户管理
         <small>用户信息</small>
     </h1>
 </section>
 <!-- Main content -->
 <section class="content">
     <div class="row">
         <div class="col-xs-12">
             <div class="box">
                 <div class="box-header">
                     <h3 class="box-title">用户列表</h3>
                 </div><!-- /.box-header -->
                 <div class="box-body">
                     <table id="example2" class="table table-bordered table-hover">
                         <thead>
                         <tr>
                             <th>用户名</th>
                             <th>密码</th>
                             <th>真实姓名</th>
                             <th>用户等级</th>
                             <th>操作</th>
                         </tr>
                         </thead>
                         <tbody>
                             #foreach(${user}in ${userList})
                             <tr>
                                 <td>${user.userName}</td>
                                 <td>${user.password}</td>
                                 <td>${user.name}</td>
                                 #if(${user.level}=="false")
                                     <td>普通会员</td>
                                 #elseif(${user.level}=="true")
                                     <td>高级会员</td>
                                 #end
                                 <td>
                                     <a href="usereditpage?id=$user.id" class="btn btn-primary">修改 </a>&nbsp<a href="userdelete?id=$user.id" class="btn btn-danger">删除 </a>
                                 </td>
                             </tr>
                             #end
                         </tbody>
                     </table>
                 </div><!-- /.box-body -->
             </div><!-- /.box -->
         </div><!-- /.col -->
     </div><!-- /.row -->
 </section><!-- /.content -->

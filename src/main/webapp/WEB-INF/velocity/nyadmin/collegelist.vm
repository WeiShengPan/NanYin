<section class="content-header">
    <h1>
        社团管理
        <small>社团信息</small>
    </h1>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">社团列表</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>社团名</th>
                            <th>所属地区</th>
                            <th>联系人</th>
                            <th>是否VIP</th>
                            <th>操作</th>
                        </tr>
                        </thead>
                        <tbody>
                            #foreach(${college}in ${collegeList})
                            <tr>
                                <td>$!{college.name}</td>
                                <td>$!{college.area.name}</td>
                                <td>$!{college.contact}</td>
                                #if(${college.vip}=="false")
                                    <td>否</td>
                                #elseif(${college.vip}=="true")
                                    <td>是</td>
                                #end
                                <td>
                                    <a href="collegeeditpage?id=$college.id">修改  </a>
                                    <a href="collegedelete?id=$college.id">删除 </a>
                                </td>
                            </tr>
                            #end
                        </tbody>
                    </table>
                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section><!-- /.content -->
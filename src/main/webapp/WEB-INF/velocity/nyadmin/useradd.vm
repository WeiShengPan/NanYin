                        <div class="box-body pad">
                            <form>
                                <!-- 用户名 -->
                                <div class="form-group">
                                    <label>用户名</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 密码 -->
                                <div class="form-group">
                                    <label>密码</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 姓名 -->
                                <div class="form-group">
                                    <label>姓名</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 性别 -->
                                <div class="form-group">
                                    <label>性别</label>
                                    <select class="form-control">
                                        <option>先生</option>
                                        <option>女士</option>
                                    </select>
                                </div>
                                <!-- 联系电话 -->
                                <div class="form-group">
                                    <label>联系电话</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 地区 -->
                                <div class="form-group">
                                    <label>原籍地</label>
                                    <select class="form-control">
                                        <option>大陆</option>
                                        <option>option 2</option>
                                        <option>option 3</option>
                                        <option>option 4</option>
                                        <option>option 5</option>
                                    </select>
                                </div>
                                <!-- 现居地 -->
                                <div class="form-group">
                                    <label>现居地</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- Email -->
                                <div class="form-group">
                                    <label>Email</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- QQ或MSN -->
                                <div class="form-group">
                                    <label>QQ或MSN</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 密保问题 -->
                                <div class="form-group">
                                    <label>密保问题</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 密保答案 -->
                                <div class="form-group">
                                    <label>密保答案</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 是否参加社团 -->
                                <div class="form-group">
                                    <label>参加参加南音社、文艺团体吗？</label>
                                    <select class="form-control">
                                        <option>否</option>
                                        <option>是</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>如果是，请填写社团名</label>
                                    <input type="text" class="form-control" placeholder="Enter ..." />
                                </div>
                                <!-- 是否成为志工 -->
                                <div class="form-group">
                                    <label>愿意成为南音网志工吗？</label>
                                    <select class="form-control">
                                        <option>否</option>
                                        <option>是</option>
                                    </select>
                                </div>
                                <!--专长-->
                                <label>专长</label>
                                <textarea id="editor1" name="editor1" rows="10" cols="80">
                                 </textarea>
                                <!-- 用户级别 -->
